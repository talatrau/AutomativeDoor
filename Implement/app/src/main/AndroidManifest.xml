<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.automativedoor">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AutomativeDoor">
        <activity android:name=".pin"></activity>
        <activity
            android:name=".ui.login.forgot_pass"
            android:label="@string/title_activity_forgot_pass" />
        <activity
            android:name=".ui.login.login"
            android:label="@string/title_activity_login" />
        <activity android:name=".feedback" />
        <activity
            android:name=".HomePage"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>